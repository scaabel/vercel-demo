{
	"npmScope": "test",
	"affected": {
		"defaultBase": "main"
	},
	"cli": {
		"defaultCollection": "@nrwl/workspace"
	},
	"implicitDependencies": {
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		".eslintrc.json": "*",
		".eslintrc.cjs": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "@nrwl/workspace/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"]
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"generators": {
		"@nrwl/react": {
			"application": {
				"babel": true
			}
		},
		"@nrwl/next": {
			"application": {
				"style": "styled-components",
				"linter": "eslint"
			}
		}
	}
}
